<%=
    javascript_include_tag "jquery-2.1.4.min",
                            "respond.min",
                            "bootstrap.min",
                            "jquery.lazyload.min",
                            :async => "",
                            :defer => "defer"
%>
<script>
    (function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
